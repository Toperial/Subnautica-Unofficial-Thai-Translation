"Sign_Aurora_Office" : "ADMINISTRATION",
"Sign_Aurora_Cargo" : "CARGO BAY 3",
"Sign_Aurora_Seamoth" : "SEAMOTH BAY",
"Sign_Aurora_DriveRoom" : "DRIVE ROOM",
"Sign_Aurora_Robotics" : "PRAWN BAY",
